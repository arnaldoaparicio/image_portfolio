Add New Picture

<%= form_with url: '/pictures', method: :post, data: {turbo: false} do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %>

  <%= form.label :description %>
  <%= form.text_field :description %>

  <%= form.label :type %>
  <%= form.text_field :type_of_picture %>

  <%= form.label :device %>
  <%= form.text_field :device %>

  <%= form.label :location %>
  <%= form.text_field :location %>

  <%= form.label :year %>
  <%= form.text_field :year %>
  
  <%= form.submit 'Submit Picture' %>
<% end %>